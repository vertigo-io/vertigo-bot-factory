<?xml version='1.0' encoding='utf-8'?>
<Context debug="0" reloadable="false" allowCasualMultipartParsing="true" docBase="/opt/tomcat/war/designer.war">
	<Resource name="jdbc/DataSource"
		auth="SERVLET"
		type="javax.sql.DataSource"
		driverClassName="org.postgresql.Driver"
		url="jdbc:postgresql:${DB_URL}?stringtype=unspecified"
		username="${DB_USER}"
		password="${DB_PWD}" />
	<Parameter name="boot.ANALYTICA_HOST" value="${ANALYTICA_HOST}" override="false"/>
	<Parameter name="boot.ANALYTICA_PORT" value="${ANALYTICA_PORT}" override="false"/>
	<Parameter name="boot.ANALYTICA_DBNAME" value="${ANALYTICA_DBNAME}" override="false"/>
</Context>