create Association ATopicTypeTopic {
	fkFieldName : "ttoCd" 
    
 	dtDefinitionA : DtTopic
	type : "*>1"
	dtDefinitionB : DtTypeTopic
	
	labelB : "Type du topic"
}


create Association ATopicChatbot {
	fkFieldName : "botId" /* NPE sans explication si ya pas */
    
