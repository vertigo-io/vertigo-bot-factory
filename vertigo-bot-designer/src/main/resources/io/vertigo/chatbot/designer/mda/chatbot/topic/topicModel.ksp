create AssociationNN AnnTopicLabel{
	tableName : "TOPIC_TOPIC_LABEL"

	dtDefinitionA : DtTopic
	dtDefinitionB : DtTopicLabel
	navigabilityA : "true"
	navigabilityB : "true"
	roleA : "Topic"
	labelA : "Topic"
	roleB : "Label"
	labelB : "Label"
}


